alias :q="exit"
alias dot="dotfiles"
alias dotfiles="$(cat $HOME/.dotfiles-command.txt)"
alias e="$VISUAL"
alias ls="ls --color=auto"
alias mkdir="mkdir -p"
alias power-suspend="$(which systemctl) suspend"
alias nide="NVIM_IDE=yes nvim"
alias py="python3"
alias qn="quicknote"
alias svdvorak="setxkbmap se -variant svdvorak -option caps:swapescape"
alias tabe="e -p"
alias u="uptime"
alias urlencode="python3 -c 'import urllib.parse as p, sys; print(p.quote(\" \".join(sys.argv[1:])), end=\"\")'"

alias -g ...="../../"
alias -g ....="../../../"
alias -g .....="../../../../"
alias -g ......="../../../../../"
alias -- -="cd -"
