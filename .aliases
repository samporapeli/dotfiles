alias ls="ls --color=auto"
alias dotfiles="$(cat $HOME/.dotfiles-command.txt)"
alias u="uptime"
alias power-suspend="sudo systemctl suspend"
alias config-i3="vim ~/.config/i3/config"
alias svdvorak="setxkbmap se -variant svdvorak -option caps:swapescape"
alias dot="dotfiles"
alias e=$VISUAL
alias py="python3"
alias qn="quicknote"
alias qnl="quicknote --latest"
alias qna="quicknote --all"
alias bat-light="BAT_THEME=ansi-light bat"
alias bat-dark="BAT_THEME=ansi-dark bat"
alias mkdir="mkdir -p"
alias :q="exit"
alias t="ttheme"
